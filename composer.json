{
    "name": "dhii/expression",
    "description": "A PHP library for creating and evaluating expressions.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dhii Team",
            "email": "development@dhii.co"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^5.3 | ^7.0",
        "dhii/expression-base": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "ptrofimov/xpmock": "^1.1",
        "dhii/php-cs-fixer-config": "dev-php-5.3",
        "codeclimate/php-test-reporter": "<=0.3.2"
    },
    "autoload": {
        "psr-4": {
            "Dhii\\Expression\\": "src"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/expression-base"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/espresso-abstract"
        }
